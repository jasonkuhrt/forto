{
  "name": "forto",
  "version": "0.0.0",
  "main": "build-umd/bundle.js",
  "module": "build-esm/bundle.js",
  "license": "MIT",
  "scripts": {
    "build:test": "parcel build puppeteer.setup.ts",
    "test:dev": "jest --watch --verbose",
    "test": "jest --coverage",
    "build": "rollup --config",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/element-resize-detector": "^1.1.0",
    "@types/jest": "^22.1.4",
    "@types/jest-environment-puppeteer": "^2.2.0",
    "@types/lodash": "^4.14.104",
    "@types/lodash-es": "^4.17.0",
    "@types/lodash.isequal": "^4.5.2",
    "@types/puppeteer": "^1.3.2",
    "@types/ramda": "^0.25.28",
    "@types/zen-observable": "^0.5.3",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-puppeteer-preset": "^3.0.1",
    "most": "^1.7.3",
    "object.assign": "^4.0.4",
    "parcel-bundler": "^1.8.1",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "puppeteer": "^1.2.0",
    "ramda": "^0.25.0",
    "rollup": "^0.59.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript": "^0.8.1",
    "ts-jest": "^22.4.1",
    "typescript": "^2.8.1"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "element-resize-detector": "^1.1.11",
    "lodash-es": "^4.17.7",
    "lodash.isequal": "^4.5.0",
    "zen-observable": "^0.8.8"
  },
  "jest": {
    "preset": "jest-puppeteer-preset",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*spec.ts$",
    "moduleFileExtensions": ["ts", "js"],
    "collectCoverageFrom": ["**/source/**/*.ts", "!**/build/**/*"]
  }
}
