{
  "name": "forto",
  "version": "1.0.0",
  "main": "build-umd/bundle.js",
  "module": "build-esm/bundle.js",
  "license": "MIT",
  "scripts": {
    "dev:test:dom": "testem",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "test:dom": "testem ci",
    "lint": "eslint source",
    "build": "rollup --config",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/element-resize-detector": "^1.1.0",
    "@types/jest": "^22.1.4",
    "@types/lodash": "^4.14.104",
    "@types/lodash-es": "^4.17.0",
    "@types/lodash.isequal": "^4.5.2",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/zen-observable": "^0.5.3",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "mocha": "^3.2.0",
    "most": "^1.7.3",
    "object.assign": "^4.0.4",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "ramda": "^0.25.0",
    "rollup": "^0.56.3",
    "rollup-plugin-closure-compiler-js": "^1.0.6",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript": "^0.8.1",
    "testem": "^1.16.0",
    "ts-jest": "^22.4.1",
    "typescript": "^2.8.1",
    "webpack": "^2.4.1"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "element-resize-detector": "^1.1.11",
    "lodash-es": "^4.17.7",
    "lodash.isequal": "^4.5.0",
    "zen-observable": "^0.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*spec.ts$",
    "moduleFileExtensions": ["ts", "js"],
    "collectCoverageFrom": ["**/source/**/*.ts", "!**/build/**/*"]
  }
}
