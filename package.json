{
  "name": "forto",
  "version": "0.0.0",
  "main": "build-umd/bundle.js",
  "types": "build-umd/Main.d.ts",
  "module": "build-esm/bundle.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build*",
    "build:test": "parcel build puppeteer.setup.ts",
    "test:watch": "jest --watch --verbose",
    "test": "jest",
    "check:types": "tsc --noEmit",
    "test:ci": "jest --config jest.config.ci.js && codecov",
    "build": "rollup --config",
    "build:watch": "nodemon --exec 'yarn build' --ext .ts --watch source",
    "dev": "yarn build:watch",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/element-resize-detector": "1.1.2",
    "@types/jest": "23.3.12",
    "@types/jest-environment-puppeteer": "2.2.1",
    "@types/lodash": "4.14.119",
    "@types/lodash-es": "4.17.1",
    "@types/lodash.isequal": "4.5.3",
    "@types/puppeteer": "1.11.1",
    "@types/ramda": "0.25.46",
    "@types/zen-observable": "0.8.0",
    "codecov": "3.1.0",
    "expect": "23.6.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "jest-puppeteer-preset": "3.7.0",
    "most": "1.7.3",
    "nodemon": "1.18.9",
    "object.assign": "4.1.0",
    "parcel-bundler": "1.11.0",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "puppeteer": "1.11.0",
    "ramda": "0.26.1",
    "rollup": "1.0.2",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-typescript2": "0.18.1",
    "ts-jest": "23.10.5",
    "typescript": "3.2.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "element-resize-detector": "^1.1.11",
    "lodash-es": "^4.17.7",
    "lodash.isequal": "^4.5.0",
    "tslib": "^1.9.3",
    "zen-observable": "^0.8.8"
  }
}
