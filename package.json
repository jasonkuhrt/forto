{
  "name": "forto",
  "version": "0.0.0",
  "main": "build-umd/bundle.js",
  "types": "build-umd/Main.d.ts",
  "module": "build-esm/bundle.js",
  "license": "MIT",
  "scripts": {
    "build:test": "parcel build puppeteer.setup.ts",
    "test:dev": "jest --watch --verbose",
    "test": "jest --coverage --verbose && codecov",
    "build": "rollup --config",
    "build:watch": "nodemon --exec 'yarn build' --ext .ts",
    "dev": "yarn build:watch",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/element-resize-detector": "1.1.0",
    "@types/jest": "23.3.2",
    "@types/jest-environment-puppeteer": "2.2.1",
    "@types/lodash": "4.14.116",
    "@types/lodash-es": "4.17.1",
    "@types/lodash.isequal": "4.5.3",
    "@types/puppeteer": "1.6.3",
    "@types/ramda": "0.25.39",
    "@types/zen-observable": "0.8.0",
    "codecov": "3.1.0",
    "husky": "1.0.1",
    "jest": "23.6.0",
    "jest-puppeteer-preset": "3.3.1",
    "most": "1.7.3",
    "nodemon": "1.18.4",
    "object.assign": "4.1.0",
    "parcel-bundler": "1.9.7",
    "prettier": "1.14.3",
    "pretty-quick": "1.7.0",
    "puppeteer": "1.8.0",
    "ramda": "0.25.0",
    "rollup": "0.66.4",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-typescript2": "0.17.0",
    "ts-jest": "23.1.4",
    "typescript": "3.1.1"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "element-resize-detector": "^1.1.11",
    "lodash-es": "^4.17.7",
    "lodash.isequal": "^4.5.0",
    "tslib": "^1.9.3",
    "zen-observable": "^0.8.8"
  },
  "jest": {
    "preset": "jest-puppeteer-preset",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverageFrom": [
      "**/source/**/*.ts",
      "!**/build/**/*"
    ]
  }
}
